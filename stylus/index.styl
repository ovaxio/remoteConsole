.rcons-wrap
  background #ccc
  width 100%
  height 25%
  min-height 60px
  // max-height 100%
  position fixed
  left 0
  bottom 0
  border-top 1px solid #b0b0b0
  z-index 10000
  box-shadow 0 1px 5px 0 rgba(0,0,0, 0.2) inset

  pre
    overflow-y auto
    overflow-x hidden
    margin-left 10px
    margin-top 10px
    margin-bottom 10px
    height 100%